<?xml version="1.0"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
                         "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
 <display-name>Archetype Created Web Application</display-name>
 <servlet>
  <servlet-name>CDType</servlet-name>
  <display-name>CDType</display-name>
  <servlet-class>controller.CDType</servlet-class>
 </servlet>
 
 <servlet>
 	<servlet-name>special</servlet-name>
 	<display-name>special</display-name>
 	<description></description>
 	<servlet-class>controller.special</servlet-class>
 </servlet>
 <servlet>
 	<servlet-name>Inter</servlet-name>
 	<display-name>Inter</display-name>
 	<description></description>
 	<servlet-class>controller.Inter</servlet-class>
 </servlet>
 <servlet-mapping>
  <servlet-name>CDType</servlet-name>
  <url-pattern>/controller.do</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
 	<servlet-name>special</servlet-name>
 	<url-pattern>/special</url-pattern>
 </servlet-mapping>
 
 <servlet-mapping>
 	<servlet-name>Inter</servlet-name>
 	<url-pattern>/Inter</url-pattern>
 </servlet-mapping>
 <welcome-file-list>
  <welcome-file>/cdDetails.jsp</welcome-file>
 </welcome-file-list>
</web-app>
