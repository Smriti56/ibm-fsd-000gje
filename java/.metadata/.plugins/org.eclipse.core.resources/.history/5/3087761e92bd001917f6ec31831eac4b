package msg;

public class Messaging {
	private String message;

	
	
	public Messaging(String message) {
		super();
		this.message = message;
	}

	public Messaging() {
		super();
	}

	/**
	 * @return the message
	 */
	public String getMessage() {
		return message;
	}

	/**
	 * @param message the message to set
	 */
	public void setMessage(String message) {
		this.message = message;
	}
	
	
	
}
