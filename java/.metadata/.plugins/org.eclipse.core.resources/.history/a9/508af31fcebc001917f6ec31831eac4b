package example;

public class MyDate {
	private int day;
	private int month;
	private int year;
	
	public boolean setDay (int d) {
			if(d<=30) {
				return true;		
			}
			else return false;
	}
	public boolean setMonth (int m) {
		if(m<=12) {
			return true;
		}
		return false;
		
	}
	public boolean setYear (int y) {
		if(y<2019) {
			return true;
		}
		return false;
		
	}
	
	day=d;
	month=m;
	year=y;
	
	
	public int getDay() {
		return day;
	}
	
public int getMonthy() {
	return month;
		
	}
public int getYear() {
	return year;
}
}

