package example;

public class Line extends Model {
	private int startPoint;
	private int endpoint;
	
	public Line() {
		super();
	}


	public int getStartPoint() {
		return startPoint;
	}

	
	public void setStartPoint(int startPoint) {
		this.startPoint = startPoint;
	}

	
	public int getEndpoint() {
		return endpoint;
	}

	
	public void setEndpoint(int endpoint) {
		this.endpoint = endpoint;
	}

	public int ClaculateLength() {
		return getEndpoint()-getStartPoint();
	}
	
	@Override
	public boolean greaterThan(Object obj1, Object obj2) {
		Line l1=(Line)obj1;
		Line l2=(Line)obj2;
		
		return l1.calculateLength()>l2.ClaculateLength();
	}

	@Override
	public boolean equalTo(Object obj1, Object obj2) {
		Line l1=(Line)obj1;
		Line l2=(Line)obj2;
		return l1.calculateLength()==l2.ClaculateLength();
		return false;
	}

	@Override
	public boolean lesserThan(Object obj1, Object obj2) {
		Line l1=(Line)obj1;
		Line l2=(Line)obj2;
		return l1.calculateLength()<l2.ClaculateLength();
		return false;
	}
	

}
